{
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "env": {
        "es6": true
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "single"],
        "no-trailing-spaces": [2, { "skipBlankLines": true }],
        "comma-dangle": ["error", "never"]
        
    }
}